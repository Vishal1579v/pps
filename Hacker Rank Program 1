#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
   
    
    int billamt,amtgiven;

    int quo,rem;

    scanf("%d%d",&billamt,&amtgiven);

    quo=billamt/amtgiven;

    rem=billamt%amtgiven;

    printf("Quotient:%d\n",quo);

    printf("Remainder:%d\n",rem);
    
    
    
   return 0;
    
    
}
